{
    "name": "danack/slim-dispatcher",
    "type": "library",
    "description": "Allows route callables to be dispatched by a supplied invoker",
    "keywords": ["auryn", "di"],
    "license": "MIT",
    "authors": [
        {
            "name": "Danack",
            "email": "Danack@basereality.com"
        }
    ],
    "config": {
      "php": "8.2.0"
    },
    "require": {
        "php": ">=8.0.0"
    },
    "require-dev": {
      "danack/php-unit-helper": "^0.5.0",
      "mockery/mockery": "^1.2.0",
      "phpunit/phpunit": "9.5.6",
      "psr/http-message": "^1.0.1",
      "slim/slim": "4.10.0",
      "squizlabs/php_codesniffer": "^3.6.0",
      "yoast/yoastcs": "1.0",
      "laminas/laminas-diactoros": "^2.6.0"
    },
    "autoload": {
        "psr-0": {
          "SlimDispatcher\\": "lib/"
        },
        "files": [
          "lib/SlimDispatcher/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "SlimDispatcherTest\\": "test/",
            "SlimDispatcherExample\\": "example/lib/"
        }
    }
}
